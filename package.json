{
  "name": "derpy-json",
  "version": "1.0.2",
  "description": "A relaxed JSON parser for TypeScript that handles various non-standard JSON formats",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "node playground/test.js"
  },
  "keywords": [
    "json",
    "parser",
    "relaxed",
    "typescript",
    "derpy"
  ],
  "author": "Stefan Ginev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choephix/derpy-json.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  }
}
